# Example Web App Environment Configuration
#
# Copy this file to .env and update the values as needed.
# Environment variables override these defaults.

# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=9999
SERVER_SCHEME=http

# Database Configuration
DB_HOST=postgres
DB_PORT=5432
DB_USER=admin
DB_PASSWORD=
DB_NAME=example_db
DB_SCHEMA=public
DB_MIN_CONNECTION_POOL_SIZE=10
DB_MAX_CONNECTION_POOL_SIZE=20
DB_CONNECTION_POOL_RECYCLE_TIME=30

# Logging Configuration
LOGGING_LEVEL=DEBUG

# Sentry Configuration (leave empty to disable)
SENTRY_DSN=

